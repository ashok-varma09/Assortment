
#include <stdio.h>

int main()
{ 
    int row ,col;
    printf("Enter size of row :");
    scanf("%d",&row);
    printf("Enter size of column :");
    scanf("%d",&col);
    int a[row][col];
              for (int i=0;i<row;i++)
              {
                  for (int j=0;j<col;j++)
                  {
                    printf("a[%d][%d]=",i,j);
                    scanf("%d",&a[i][j]);
                  }
              }
// transponse 
              for (int i=0;i<row;i++)
              {
                  for (int j=0;j<col;j++)
                  {
                    printf("%d ",a[j][i]);  
                  }
                  printf("\n");
              }
    return 0;
}
